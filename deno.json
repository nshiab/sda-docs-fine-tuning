    "@nshiab/journalism": "jsr:@nshiab/journalism@^1.34.6",
    "@nshiab/simple-data-analysis": "jsr:@nshiab/simple-data-analysis@^5.6.31",
    "@observablehq/plot": "npm:@observablehq/plot@^0.6.17"
  }
